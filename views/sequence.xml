<odoo>
    <data>
        <record id="sequence_room" model="ir.sequence">
            <field name="name">Room</field>
            <field name="code">openacademy.room</field>
            <field name="active">TRUE</field>
            <field name="prefix">R</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_session" model="ir.sequence">
            <field name="name">Session</field>
            <field name="code">openacademy.session</field>
            <field name="active">TRUE</field>
            <field name="prefix">SS</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>